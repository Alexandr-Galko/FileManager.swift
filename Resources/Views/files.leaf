<!doctype html>
<html lang="ru">
  <head>
  	<meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="/css/bootstrap.css" rel="stylesheet">
    <title>File manager</title>
  </head>
  <body>
  	<h1>File manager</h1>
  	Path: #(path)
  	<div class="container">
  		<table class="table table-striped">
  		#loop(files, "file") {
  		  <tr>
            <td>
              #(file)
            </td>
          </tr>
		  }
  		</table>
  	</div>
  </body>
</html>